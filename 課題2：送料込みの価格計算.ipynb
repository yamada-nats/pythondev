   "source": [
    "price = int(input(\"税抜価格を入力してください：\"))\n",
    "price_inctax = round(price *1.1)\n",
    "\n",
    "if price_inctax >= 2000:\n",
    "    print(\"送料は無料です。\")\n",
    "    total_price = price_inctax\n",
    "else:\n",
    "    print(\"送料として350円かかります。\")\n",
    "    total_price = price_inctax +350\n",
    "\n",
    "print(f\"送料込みの価格は{total_price}円です。\")"
   ]
  }
